<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
 <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
     android:background="@color/white">

     <androidx.cardview.widget.CardView
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         app:cardCornerRadius="5dp"
         android:layout_margin="3dp"
         android:elevation="5dp"
         android:backgroundTint="@color/white">

         <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_margin="3dp"
             android:orientation="horizontal">

             <LinearLayout
                 android:layout_width="0dp"
                 android:layout_height="wrap_content"
                 android:layout_weight="3"
                 android:orientation="vertical"
                 android:weightSum="2 ">

                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight=".5"
                     android:orientation="horizontal"
                     android:weightSum="2">

                     <TextView
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight=".55"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text="Entry Time"
                         android:textColor="@color/black"
                         android:textSize="@dimen/fourteen_sp_text_size"/>

                     <TextView
                         android:id="@+id/entryTime"
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight="1.4"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text=":"
                         android:textSize="@dimen/thirteen_sp_size"
                         android:textColor="@color/gray" />


                 </LinearLayout>
                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight=".5"
                     android:orientation="horizontal"
                     android:weightSum="2">

                     <TextView
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight=".55"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text="Name"
                         android:textColor="@color/black"
                         android:textSize="@dimen/fourteen_sp_text_size" />

                     <TextView
                         android:id="@+id/name"
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight="1.4"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text=":"
                         android:textSize="@dimen/thirteen_sp_size"
                         android:textColor="@color/gray" />


                 </LinearLayout>

                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight=".5"
                     android:orientation="horizontal"
                     android:weightSum="2">

                     <TextView
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight=".55"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text="Display Name"
                         android:textColor="@color/black"
                         android:textSize="@dimen/fourteen_sp_text_size"/>

                     <TextView
                         android:id="@+id/displayName"
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight="1.4"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text=":"
                         android:textSize="@dimen/thirteen_sp_size"
                         android:textColor="@color/gray" />


                 </LinearLayout>

                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight=".5"
                     android:orientation="horizontal"
                     android:weightSum="2">

                     <TextView
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight=".55"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text="Process Type"
                         android:textColor="@color/black"
                         android:textSize="@dimen/fourteen_sp_text_size" />

                     <TextView
                         android:id="@+id/processTpe"
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight="1.4"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text=":"
                         android:textSize="@dimen/thirteen_sp_size"
                         android:textColor="@color/gray"  />


                 </LinearLayout>

                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight=".5"
                     android:orientation="horizontal"
                     android:weightSum="2">

                     <TextView
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight=".55"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text="Miller Type"
                         android:textColor="@color/black"
                         android:textSize="@dimen/fourteen_sp_text_size"/>

                     <TextView
                         android:id="@+id/millerType"
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight="1.4"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text=":"
                         android:textSize="@dimen/thirteen_sp_size"
                         android:textColor="@color/gray"  />


                 </LinearLayout>

                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight=".5"
                     android:orientation="horizontal"
                     android:weightSum="2">

                     <TextView
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight=".55"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text="Capacity"
                         android:textColor="@color/black"
                         android:textSize="@dimen/fourteen_sp_text_size" />

                     <TextView
                         android:id="@+id/capacity"
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight="1.4"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text=":"
                         android:textSize="@dimen/thirteen_sp_size"
                         android:textColor="@color/gray"  />


                 </LinearLayout>

                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight=".5"
                     android:orientation="horizontal"
                     android:weightSum="2">

                     <TextView
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight=".55"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text="Country name"
                         android:textColor="@color/black"
                         android:textSize="@dimen/fourteen_sp_text_size" />

                     <TextView
                         android:id="@+id/countyName"
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight="1.4"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text=":"
                         android:textSize="@dimen/thirteen_sp_size"
                         android:textColor="@color/gray" />


                 </LinearLayout>


                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight=".5"
                     android:orientation="horizontal"
                     android:weightSum="2">

                     <TextView
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight=".55"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text="Division Name"
                         android:textColor="@color/black"
                         android:textSize="@dimen/fourteen_sp_text_size" />

                     <TextView
                         android:id="@+id/division"
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight="1.4"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text=":"
                         android:textSize="@dimen/thirteen_sp_size"
                         android:textColor="@color/gray"  />


                 </LinearLayout>

                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight=".5"
                     android:orientation="horizontal"
                     android:weightSum="2">

                     <TextView
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight=".55"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text="District Name"
                         android:textColor="@color/black"
                         android:textSize="@dimen/fourteen_sp_text_size" />

                     <TextView
                         android:id="@+id/district"
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight="1.4"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text=":"
                         android:textSize="@dimen/thirteen_sp_size"
                         android:textColor="@color/gray"  />


                 </LinearLayout>
                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_weight=".5"
                     android:orientation="horizontal"
                     android:weightSum="2">

                     <TextView
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight=".55"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text="Upazila"
                         android:textColor="@color/black"
                         android:textSize="@dimen/fourteen_sp_text_size"/>

                     <TextView
                         android:id="@+id/upazilla"
                         android:layout_width="0dp"
                         android:layout_height="match_parent"
                         android:layout_weight="1.4"
                         android:gravity="center_vertical"
                         android:singleLine="true"
                         android:text=":"
                         android:textSize="@dimen/thirteen_sp_size"
                         android:textColor="@color/gray"  />


                 </LinearLayout>




             </LinearLayout>


         </LinearLayout>

     </androidx.cardview.widget.CardView>


    </LinearLayout>
</layout>